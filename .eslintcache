 from './data'\r\nimport reducer from './reducer'\r\n\r\n\r\nconst url = 'https://course-api.com/react-useReducer-cart-project'\r\nconst CartContext = React.createContext()\r\n\r\nconst initialState = {\r\n  loading: true,\r\n  items: cartItems,\r\n  totalAmount: 0,\r\n  totalPrice: 0\r\n}\r\n\r\nconst CartProvider = ({children}) => {\r\n\r\n  \r\n  const [state, dispatch] = useReducer(reducer, initialState)\r\n  \r\n  const fetchData = async () => {\r\n    const resp = await fetch(url);\r\n    const items = await resp.json();\r\n    dispatch({type: 'FETCH_ITEMS', payload: items})\r\n    dispatch({type: 'LOADING'})\r\n  }\r\n\r\n  useEffect(() => {\r\n    fetchData();\r\n  }, [])\r\n\r\n\r\n  const removeAll = () => {\r\n    dispatch({type: 'REMOVE_ALL'})\r\n  }\r\n\r\n  const removeItem = (id) => {\r\n    dispatch({type: 'REMOVE_ITEM', payload: id})\r\n  }\r\n\r\n  const increment = (id) => {\r\n    dispatch({type: 'INCREMENT', payload: id})\r\n  }\r\n\r\n  const decrement = (id) => {\r\n    dispatch({type: 'DECREMENT', payload: id})\r\n  }\r\n\r\n  useEffect(() => {\r\n    dispatch({type: 'TOTAL_AMOUNT_AND_PRICE'});\r\n  }, [state.items])\r\n\r\n  return (\r\n    <CartContext.Provider value={{\r\n        ...state,\r\n        removeAll,\r\n        increment,\r\n        decrement,\r\n        removeItem,\r\n      }}>\r\n      {children}\r\n    </CartContext.Provider>\r\n  )\r\n\r\n}\r\n\r\n// make sure use\r\nexport const useCartContext = () => {\r\n  return useContext(CartContext)\r\n}\r\n\r\nexport { CartContext, CartProvider }","C:\\Users\\Ivan\\Desktop\\react-fun\\src\\components\\cart\\reducer.js",[],"C:\\Users\\Ivan\\Desktop\\react-fun\\src\\components\\Loading.js",[],"C:\\Users\\Ivan\\Desktop\\react-fun\\src\\components\\cart\\data.js",[],"C:\\Users\\Ivan\\Desktop\\react-fun\\src\\components\\Error.js",[],"C:\\Users\\Ivan\\Desktop\\react-fun\\src\\components\\Home.js",[],"C:\\Users\\Ivan\\Desktop\\react-fun\\src\\components\\Components.js",[],"C:\\Users\\Ivan\